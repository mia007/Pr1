@CHARSET "UTF-8";

html {
	font-size: 20px;
	height: 100%
}

body {
	font-family: sans-serif;
	margin: 0;
	background-color: #afe3ea;
	font-size: 18px;
	color: #3d7291;
	min-height: 100%;
}

button, input, table, a, label {
	font-family: sans-serif;
	font-size: 18px;
}
form {
	margin: 0
}
select{
	font-size: 17.5px;
	vertical-align: middle;
}
input{
	margin-top:2rem;
}
input, select {
	box-sizing: border-box;
	border: 1px solid #ffffff;
	border-radius: 2px;
	padding: 1rem 1rem;
	background-color: #eff8f7;
	box-shadow: 0 1px 1px 0 rgba(75, 75, 75, .15) inset, 0 2px 0 0 #408080;
	margin-right: 1rem;
}

.table-hover th, .table-hover td{
	width: 40%;
	background-color: #eff8f7;
	border-radius: 2px;
	border: 1px solid #400080;
	box-shadow: 0 1px 1px 0 rgba(75, 75, 75, .15) inset, 0 2px 0 0 #408080;
}
.table-bordered, .panel>.table-responsive>.table-bordered {
	background-color: #eff8f7;
	border-radius: 2px;
	border: 1px solid #ffffff;
	box-shadow: 0 1px 1px 0 rgba(75, 75, 75, .15) inset, 0 2px 0 0 #408080;
	width: 100%;
	margin-bottom:2rem;
	margin-top: 2rem;
}
@media screen {
	html {
		font-size: 11.5px
	}
	body, button, input {
		font-size: 20px;
	}
}

button:hover {
	opacity: 0.8;
}

.pagealign {
	text-align: center;
