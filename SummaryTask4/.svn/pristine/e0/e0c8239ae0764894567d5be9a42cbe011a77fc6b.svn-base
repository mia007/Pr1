}

.fine-button {
	box-sizing: border-box;
	border-radius: 1px;
	display: inline-block;
	overflow: hidden
}

.fine-button:hover {
	opacity: 0.8;
}

.fine-button button, .fine-button input, .fine-button>a {
	box-sizing: border-box;
	text-decoration: none;
	display: inline-block;
	color: #e8f1f7;
	background-color: #3d7291;
	background: linear-gradient(0deg, #066772 0, #3d7291 100%);
	border-style: solid;
	border-radius: 3px;
	border-width: 1px;
	border-color: #A6D2EF;
	width: 100%;
	height: 100%;
	cursor: pointer
}

.fine-button.default {
	height: 3rem
}

.fine-button.default a, .fine-button.default button {
	font-size: 1.5rem;
	font-weight: 350;
	line-height: 3rem;
	padding: 0 1rem;
	top: 5px;
}

/* The side navigation menu */
.sidenav {
	height: 100%; /* 100% Full-height */
	width: 0; /* 0 width - change this with JavaScript */
	position: fixed; /* Stay in place */
	z-index: 1; /* Stay on top */
	top: 0;
	left: 0;
	background-color: #3d7291;
	background: linear-gradient(0deg, #066772 0, #3d7291 100%);
	overflow-x: hidden; /* Disable horizontal scroll */
	padding-top: 20px; /* Place content 60px from the top */
	transition: 0.3s;
	background: linear-gradient(0deg, #066772 0, #3d7291 100%);
	/* 0.5 second transition effect to slide in the sidenav */
}

.sidenav .top .lang {
	display: inline;
	vertical-align: middle
}

.sidenav .top .lang a {
	display: inline-block;
	width: 15px;
	height: 10px;
	line-height: 1.5rem;
	margin-left: 1rem;
	padding-right: 50px;
}

.sidenav .top .lang a.cur {
	background-color: #fff;
	color: #265c76;
}
/* The navigation menu links */
.sidenav a {
	padding: 8px 8px 15px 15px;
	text-decoration: none;
	font-size: 25px;
	color: #fff;
	display: block;
	transition: 0.3s;
}

/* When you mouse over the navigation links, change their color */
.sidenav a:hover, .offcanvas a:focus {
	color: #80ffff;
}

/* Position and style the close button (top right corner) */
.sidenav .closebtn {
	position: absolute;
	top: 0;
	right: 20px;
	font-size: 36px;
	margin-left: 5px;
}

